{
  "name": "embeddebale-chat",
  "version": "0.0.1",
  "description": "embeddable chat for NRK apps",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "mocha src/**/_spec/*.spec.js* --compilers js:babel-core/register --reporter nyan",
    "compress": "sh ./scripts/compress.sh",
    "start": "npm run compress && node servers/dist.js",
    "clean": "rimraf public",
    "start:dev": "node servers/dev.js",
    "copy:dist": "cp index.html public/",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack && npm run copy:dist",
    "lint": "eslint src"
  },
  "license": "NRK",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "0.18.0",
    "eslint": "^1.10.3",
    "eslint-config-nrk": "git+ssh://git@github.com:nrkno/eslint-config-nrk.git#v0.0.4",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-svgstore-plugin": "^2.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "express": "^4.13.3",
    "http-static-gzip-regexp": "0.0.2",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "superagent": "^1.8.0"
  }
}
